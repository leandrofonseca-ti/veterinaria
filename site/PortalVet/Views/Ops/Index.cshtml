@model  PortalVet.Models.ViewOps
@{
    Layout = null;
    ViewBag.Title = "Permission";
}
<!DOCTYPE html>
<html>
<head lang="en">
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>:: Manager ::</title>
    <link href="@Url.Content("~/Content/img/icon/apple-touch-icon-144x144.png")" rel="apple-touch-icon" type="image/png" sizes="144x144">
    <link href="@Url.Content("~/Content/img/icon/apple-touch-icon-114x114.png")" rel="apple-touch-icon" type="image/png" sizes="114x114">
    <link href="@Url.Content("~/Content/img/icon/apple-touch-icon-72x72.png")" rel="apple-touch-icon" type="image/png" sizes="72x72">
    <link href="@Url.Content("~/Content/img/icon/apple-touch-icon-57x57.png")" rel="apple-touch-icon" type="image/png">
    <link href="@Url.Content("~/Content/img/icon/favicon-32x32.png")" rel="icon" type="image/png">
    <link href="@Url.Content("~/Content/img/icon/favicon.ico")" rel="shortcut icon">
    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    <link rel="stylesheet" href="@Url.Content("~/Content/css/separate/pages/error.min.css")">
    <link rel="stylesheet" href="@Url.Content("~/Content/css/lib/font-awesome/font-awesome.min.css")">
    <link rel="stylesheet" href="@Url.Content("~/Content/css/lib/bootstrap/bootstrap.min.css")">
    <link rel="stylesheet" href="@Url.Content("~/Content/css/main.css")">

    <style>
    body { background-color: transparent !important; }
        html {
            /*background: url('Url.Content("~/Content/Images/bg-sohtec1.jpg")') no-repeat center center fixed;*/
            background: -webkit-gradient(linear, left top, right top, from(#000000), to(#0082c6)) no-repeat;
            background: -moz-linear-gradient(left, #000000, #0082c6);
            filter: progid:DXImageTransform.Microsoft.Gradient(GradientType=1, StartColorStr='#000000', EndColorStr='#0082c6');
            -webkit-background-size: cover;
            -moz-background-size: cover;
            -o-background-size: cover;
            background-size: cover;
        }
    .panel { background-color: rgba(255, 255, 255, 0.50) !important; }
    @@media (min-width: 1024px) {
        html {
            /*background: url('Url.Content("~/Content/Images/bg-sohtec1.jpg")') no-repeat center center fixed;*/
            background: -webkit-gradient(linear, left top, right top, from(#000000), to(#0082c6)) no-repeat;
            background: -moz-linear-gradient(left, #000000, #0082c6);
            filter: progid:DXImageTransform.Microsoft.Gradient(GradientType=1, StartColorStr='#000000', EndColorStr='#0082c6');
            -webkit-background-size: cover;
            -moz-background-size: cover;
            -o-background-size: cover;
            background-size: cover;
        }
    }
</style>

</head>
<body>
    <div class="page-error-box">
        <div class="error-code" style="text-align: justify;color: #00a8ff;border-color: #00a8ff;">Ops!</div>        
        <div><p style="text-align:left;margin:10px 10px 10px 0px;font-size:18px"><strong>Sessão expirou, e tente entrar novamente, <br /><a href="@Model.URL" style="color: #00a8ff;border-color: #00a8ff;"><u>clicando aqui!!</u></a></strong></p></div>

        @*<img src="@Url.Content("~/Content/img/icon/favicon-96x96.png")" />*@

        <div style="display:none">
            <br /><br />
            @if (Session["DataMessage"] != null)
            {
                @Session["DataMessage"].ToString();
            }
        </div>
    </div>
</body>
</html>
