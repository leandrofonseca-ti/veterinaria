<!DOCTYPE html>
<html>
<head lang="en">
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no" />
    <meta http-equiv="x-ua-compatible" content="ie=edge" />
    <title>:: Portal Vet ::</title>

    <link href="~/Content/css/animate.css" rel="stylesheet" />
    <link rel="apple-touch-icon-precomposed" sizes="57x57" href="@Url.Content("~/Content/img/icon/apple-touch-icon-57x57.png")" />
    <link rel="apple-touch-icon-precomposed" sizes="114x114" href="@Url.Content("~/Content/img/icon/apple-touch-icon-114x114.png")" />
    <link rel="apple-touch-icon-precomposed" sizes="72x72" href="@Url.Content("~/Content/img/icon/apple-touch-icon-72x72.png")" />
    <link rel="apple-touch-icon-precomposed" sizes="144x144" href="@Url.Content("~/Content/img/icon/apple-touch-icon-144x144.png")" />
    <link rel="apple-touch-icon-precomposed" sizes="60x60" href="@Url.Content("~/Content/img/icon/apple-touch-icon-60x60.png")" />
    <link rel="apple-touch-icon-precomposed" sizes="120x120" href="@Url.Content("~/Content/img/icon/apple-touch-icon-120x120.png")" />
    <link rel="apple-touch-icon-precomposed" sizes="76x76" href="@Url.Content("~/Content/img/icon/apple-touch-icon-76x76.png")" />
    <link rel="apple-touch-icon-precomposed" sizes="152x152" href="@Url.Content("~/Content/img/icon/apple-touch-icon-152x152.png")" />
    <meta name="application-name" content="Portal Vet" />
    <meta name="msapplication-TileColor" content="#FFFFFF" />
    <meta name="msapplication-TileImage" content="@Url.Content("~/Content/img/icon/mstile-144x144.png")" />
    <meta name="msapplication-square70x70logo" content="@Url.Content("~/Content/img/icon/mstile-70x70.png")" />
    <meta name="msapplication-square150x150logo" content="@Url.Content("~/Content/img/icon/mstile-150x150.png")" />
    <meta name="msapplication-wide310x150logo" content="@Url.Content("~/Content/img/icon/mstile-310x150.png")" />
    <meta name="msapplication-square310x310logo" content="@Url.Content("~/Content/img/icon/mstile-310x310.png")" />
    <meta name="theme-color" content="#ffffff" />

    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->

    <link rel="stylesheet" href="@Url.Content("~/Content/css/separate/pages/login.min.css")" />
    <link rel="stylesheet" href="@Url.Content("~/Content/css/lib/font-awesome/font-awesome.min.css")" />


    <link rel="stylesheet" href="@Url.Content("~/Content/css/separate/vendor/bootstrap-select/bootstrap-select.min.css")">
    <link rel="stylesheet" href="@Url.Content("~/Content/css/separate/vendor/select2.min.css")">
    <link rel="stylesheet" href="@Url.Content("~/Content/css/lib/font-awesome/font-awesome.min.css")">


    <link rel="stylesheet" href="@Url.Content("~/Content/css/lib/bootstrap/bootstrap.min.css")" />
    <link rel="stylesheet" href="@Url.Content("~/Content/css/main.css")" />
    <link rel="stylesheet" href="@Url.Content("~/Content/css/separate/vendor/blockui.min.css")" />


    @RenderSection("styles", required: false)
</head>
<body>
    @Html.Hidden("hdnFull")
    <input type="hidden" id="UrlBase" value="@ViewData["rootUrl"]" />
    <input type="hidden" id="UrlBaseImob" value="@PortalVet.Helper.AppSettings.UrlRootAdmin" />
    <input type="hidden" id="UrlDominio" value="@ViewData["rootUrlDominio"]" />

    <div class="page-center">
        <div class="page-center-in">
            <div class="container-fluid">
                @RenderBody()
            </div>
        </div>
    </div><!--.page-center-->
    @*@Scripts.Render("~/bundles/jslogin")*@

    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.6.10/angular.min.js"></script>

    <script src="@Url.Content("~/Content/js/lib/jquery/jquery-1.12.4.js?t=" + Guid.NewGuid())"></script>
    <script type="text/javascript" src="@Url.Content("~/Scripts/_JSERVICE.js?t=" + Guid.NewGuid())"></script>
    <script type="text/javascript" src="@Url.Content("~/Content/js/lib/input-mask/jquery.mask.min.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Content/js/lib/match-height/jquery.matchHeight.min.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Content/js/lib/blockUI/jquery.blockUI.js")"></script>

    <script src="@Url.Content("~/Content/js/lib/bootstrap/bootstrap.min.js")"></script>
    <script src="@Url.Content("~/Content/js/lib/bootstrap-select/bootstrap-select.min.js")"></script>
    <script src="@Url.Content("~/Content/js/lib/select2/select2.full.min.js")"></script>

    <script>



        $(function () {
            $('.page-center').matchHeight({
                target: $('html')
            });

            $(window).resize(function () {
                setTimeout(function () {
                    $('.page-center').matchHeight({ remove: true });
                    $('.page-center').matchHeight({
                        target: $('html')
                    });
                }, 100);
            });



            console.log("URL hostname: " + window.location.hostname);

            console.log("URL host: " + window.location.host);

            console.log("URL href: " + window.location.href);

            console.log("URL ancestorOrigins: " + window.location.ancestorOrigins);

            console.log("URL origin: " + window.location.origin);

            console.log("URL pathname: " + window.location.pathname);

            console.log("URL port: " + window.location.port);

            console.log("URL protocol: " + window.location.protocol);



            var browser = "";
            var ua = navigator.userAgent.toLowerCase();
            if (ua.indexOf("safari") !== -1) {
                if (ua.indexOf("chrome") > -1) {
                    browser = "chrome";
                }
                else {
                    browser = "safari";
                }
            }
            else {
                browser = "geral";
            }

            console.log("browser : " + browser);

        });
    </script>
    @RenderSection("scripts", required: false)
</body>
</html>