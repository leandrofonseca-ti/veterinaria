<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta name="generator" content="" />
    <title>:: WebImagem ::</title>

    <meta name="keywords" content="raio-x, radiologia, diagnostico, imagem, animais, veterinario" />
    <meta name="description" content="Diagnóstico por Imagem Veterinário • Unidade móvel • Atendimento em clínicas e domicílios • Porto Alegre e Região Metropolitana.">
    <meta name="author" content="">

    <!-- Favicon -->
    <link rel="shortcut icon" href="img/ico-2019.svg" type="image/x-icon" />
    <link rel="apple-touch-icon" href="img/ico-2019.svg">




    @Styles.Render("~/Content/Scorilo/css/bootstrap.min.css")
    <link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.6.3/css/font-awesome.min.css" rel="stylesheet">
    @Styles.Render("~/Content/Scorilo/css/style.css")
    <link href="https://fonts.googleapis.com/css?family=Dosis:200,300,400,500,600,700" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Roboto:200,300,400,500,600,700" rel="stylesheet">
</head>
<body>

    <!-- HEADER =============================-->
    <header class="item header margin-top-0">
        <div class="wrapper">
            <nav role="navigation" class="navbar navbar-white navbar-embossed navbar-lg navbar-fixed-top">
                <div class="container">
                    <div class="navbar-header">
                        <button data-target="#navbar-collapse-02" data-toggle="collapse" class="navbar-toggle" type="button">
                            <i class="fa fa-bars"></i>
                            <span class="sr-only">Toggle navigation</span>
                        </button>
                        <a href="#" class="navbar-brand brand"> Web Imagem </a>
                    </div>
                    <div id="navbar-collapse-02" class="collapse navbar-collapse">
                        <ul class="nav navbar-nav navbar-right">
                            @*<li class="propClone"><a href="index.html">Home</a></li>
                            <li class="propClone"><a href="shop.html">Shop</a></li>
                            <li class="propClone"><a href="product.html">Product</a></li>
                            <li class="propClone"><a href="checkout.html">Checkout</a></li>*@
                            <li class="propClone">@Html.ActionLink("Área do Cliente", "Index", "Admin", new { area = "" }, null)</li>
                        </ul>
                    </div>
                </div>
            </nav>
            <div class="container">
                <div class="row">
                    <div class="col-md-12 text-center">
                        <div class="text-homeimage">
                            <div class="maintext-image" data-scrollreveal="enter top over 1.5s after 0.1s">
                                Med Vet especializada
                            </div>
                            <div class="subtext-image" data-scrollreveal="enter bottom over 1.7s after 0.3s">
                                Diagnóstico por imagem
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </header>


    @RenderBody()
    <!-- SCRIPTS =============================-->
    @Scripts.Render("~/Content/Scorilo/js/jquery-.js")
    @Scripts.Render("~/Content/Scorilo/js/bootstrap.min.js")
    @Scripts.Render("~/Content/Scorilo/js/anim.js")
    @RenderSection("scripts", required: false)
    <script>
        //----HOVER CAPTION---//
        jQuery(document).ready(function ($) {
            $('.fadeshop').hover(
                function () {
                    $(this).find('.captionshop').fadeIn(150);
                },
                function () {
                    $(this).find('.captionshop').fadeOut(150);
                }
            );
        });
    </script>

</body>
</html>