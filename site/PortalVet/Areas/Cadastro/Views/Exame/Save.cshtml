                                        <div class="col-md-12">

                                            <fieldset class="form-group">
                                                @*<label for="txtArquivos" class="form-label">Anexar arquivo(s)</label>*@
                                                <div class="divControles">
                                                    <div class="row uploadArquivos">
                                                        <div class="col-sm-6">
                                                            <div class="input-group mt-2 clsImages" onclick="FILECONTROL.OpenModalImage('@Url.Action("UploadFileControl", "Exame", new { @area = "Cadastro"})')">
                                                                <div class="input-group-prepend">
                                                                    <span class="input-group-text" id="inputGroupPrepend"><i class="font-icon font-icon-clip"></i></span>
                                                                </div>
                                                                <input type="text" id="txtArquivos" name="txtArquivos" class="form-control form-control-sm" disabled readonly placeholder="Anexar arquivo(s)" aria-describedby="inputGroupPrepend" />
                                                            </div>
                                                            <div class="row-fluid">
                                                                <div class="span6">
                                                                    <div class="control-group">
                                                                        <div class="controls">
                                                                            <input type="hidden" id="hdnPathControl" value="@Url.Content("~/Cadastro/Exame")" />
                                                                            <input type="hidden" id="hdnTotalFotos" value="0" />
                                                                            <div class="list-group" id="listImages" style="list-style-type: none; margin-left: 0px !important">
                                                                                @{
                                                                                    var ct = 0;
                                                                                    foreach (var file in arquivos)
                                                                                    {
                                                                                        ct++;
                                                                                        <div class="list-group-item list-group-item-success mt-1">
                                                                                            <div class="row">
                                                                                                <div class="col-10 text-left">
                                                                                                    @*<a href="javascript:;" data-id="@ct">*@
                                                                                                    <a href="javascript:;" title="Edição de Imagem" data-id="@ct" onclick="FILECONTROL.OpenEditImage(this,'@Url.Content($"~/upload/exames/{Model.EmpresaId}/{Model.ID}/{file.OriginalFileName}")',@Model.EmpresaId,@Model.ID,'@file.OriginalFileName')">
                                                                                                        <img id="picture_@ct" src="@Url.Content($"~/upload/exames/{Model.EmpresaId}/{Model.ID}/{file.OriginalFileName}")" style="height:100px;" />
                                                                                                        @*<span class="mb-1" style="font-size: 11px;">&nbsp;@file.OriginalFileName</span>*@
                                                                                                    </a>
                                                                                                </div>
                                                                                                <div class="col-2">
                                                                                                    <button type="button" class="btn btn-sm btn-inline btn-danger clsImages" title="remover" onclick="FILECONTROL.RemoveFile('@file.ID')"><i class="font-icon font-icon-del"></i></button>
                                                                                                    <br />
